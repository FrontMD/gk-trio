    const defaultTabsProps = {
        classes: '',
        tabs: [
            {
                name: 'Вкладка 1',
                icon: ''
            },
            {
                name: 'Вкладка 2',
                icon: ''
            },
        ],
    }

mixin tabs-block(props= defaultTabsProps)
    -
        const checkProps = {
            classes: props.classes ? props.classes : defaultTabsProps.classes,
            tabs: props.tabs ? props.tabs : defaultTabsProps.tabs
        }

    section.tabs-block
        .section__container.product-detailed__container
            .gkt-container
                .gkt-container__inner.gkt-container__inner--bg
                    .tabs-block__tabs.scrollbar.scrollbar--small(data-js="tabsBlockList")
                        .tabs-block__list
                            .tabs-block__tab.active(data-index="0" data-js="tabsBlockTab")
                                svg
                                    use(xlink:href=`img/sprites/sprite.svg#`)
                                span Описание
                            .tabs-block__tab(data-index="1" data-js="tabsBlockTab")
                                svg
                                    use(xlink:href=`img/sprites/sprite.svg#`)
                                span Характеристики
                            .tabs-block__tab(data-index="2" data-js="tabsBlockTab")
                                svg
                                    use(xlink:href=`img/sprites/sprite.svg#`)
                                span Документы

                    .tabs-block__slides
                        .tabs-block__slide.active(data-js="tabsBlockSlide")
                            .product-detailed__desc.text-editor#lineDesc
                                h3 Особенности линейки

                                ul 
                                    li Эргономичный пульт управления;
                                    li Управление машиной и испытаниями осуществляется через программу с ПК или ноутбука;
                                    li Зарегистрированная торговая марка, защита от подделок;
                                    li Точность позиционирования траверсы;
                                    li Плавный ход захватов и приспособлений

                                hr

                                h3#completeSet Заводская комплектация

                                ul
